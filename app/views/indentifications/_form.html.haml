= form_for @indentification do |f|
  - if @indentification.errors.any?
    #error_explanation
      %h2= "#{pluralize(@indentification.errors.count, "error")} prohibited this indentification from being saved:"
      %ul
        - @indentification.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :name
    = f.text_field :name
  .actions
    = f.submit 'Save'
