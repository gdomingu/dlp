= form_for @identification do |f|
  - if @identification.errors.any?
    #error_explanation
      %h2= "#{pluralize(@identification.errors.count, "error")} prohibited this identification from being saved:"
      %ul
        - @identification.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :name
    = f.text_field :name
  .actions
    = f.submit 'Save'
